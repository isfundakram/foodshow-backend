{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [ "get", "post" ],
      "route": "negotiate"
    },
    { "type": "http", "direction": "out", "name": "$return" },
    {
      "type": "signalRConnectionInfo",
      "name": "connectionInfo",
      "hubName": "booth",
      "direction": "in",
      "connectionStringSetting": "AzureSignalRConnectionString"
    }
  ]
}